<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical" >
    <LinearLayout
    android:id="@+id/address_lv_item_all_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    android:orientation="vertical">
        <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
			<LinearLayout
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			android:orientation="horizontal"
			android:layout_marginLeft="10dp"
			android:gravity="center_vertical|left">
			    
			    <TextView
			    android:id="@+id/address_lv_item_name"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="dfafdfasfdsafads"
			    android:textSize="@dimen/h2"/>
			    
			</LinearLayout>            
            <LinearLayout
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			android:orientation="horizontal"
			android:layout_marginRight="10dp"
			android:gravity="center_vertical|right">
			    
			     <TextView
			    android:id="@+id/address_lv_item_phone"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="15468496498"
			    android:textSize="@dimen/h2"/>
			</LinearLayout>  
        </LinearLayout>
        
        <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal">
            
            <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="7"
            android:orientation="horizontal">
                <TextView
	            android:id="@+id/address_lv_item_details"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            />
            </LinearLayout>
            
            <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="right"
            android:layout_weight="1">
                
                <ImageView
                android:id="@+id/address_lv_item_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_address_select"
                android:visibility="gone"/>
                
                <ImageView
                android:id="@+id/address_lv_item_update"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_address_update"
                android:visibility="gone"/>
                
            </RelativeLayout>
            
        </LinearLayout>
    </LinearLayout>
	<View
	android:layout_width="match_parent"
	android:layout_height="0.5dp"
	android:background="@color/title_hor_line"/>
</LinearLayout>
