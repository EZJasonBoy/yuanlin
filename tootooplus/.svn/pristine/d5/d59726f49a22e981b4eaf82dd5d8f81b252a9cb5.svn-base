package com.ninetowns.tootooplus.bean;

import java.io.Serializable;
import java.util.List;

/**
 * @ClassName: PersonInfoDetailBean
 * @Description: TODO(描述这个类的作用)
 * @author zhou
 * @date 2015-4-17 下午2:19:44
 * 
 */

public class PersonInfoDetailBean implements Serializable {

	public String Status;
	public List<HelpInfo> HelpInfoList;

	public UserInfo UserInfo;

	@Override
	public String toString() {
		return "PersonInfoDetailBean [Status=" + Status + ", HelpInfoList="
				+ HelpInfoList.toString() + ", UserInfo=" + UserInfo.toString() + "]";
	}

	public static class HelpInfo {
		public String HelpId;
		public String HelpTitle;
		public String Images;
		public String Content;
		@Override
		public String toString() {
			return "HelpInfo [HelpId=" + HelpId + ", HelpTitle=" + HelpTitle
					+ ", Images=" + Images + ", Content=" + Content + "]";
		}

	}

	public static class UserInfo {
		/**
		 * @Fields info_userId : 用户id
		 */
		public String UserId;

		/**
		 * @Fields UserName : 用户名
		 */
		public String UserName;

		/**
		 * @Fields LogoUrl :头像
		 */
		public String LogoUrl;

		/**
		 * @Fields TCurrency : 等级的点数
		 */
		public String TCurrency;
		/**
		 * @Fields PublishStory : 暂时用不上 不知道干嘛使得
		 */
		public String PublishStory;
		/**
		 * @Fields BusinessStatus : 是否为商家 1为商家
		 */
		public String BusinessStatus;
		/**
		 * @Fields UserGrade : 用户vip等级
		 */
		public String UserGrade;

		/**
		 * @Fields Verified : TODO(描述这个变量表示什么)
		 */
		public String Verified;
		/**
		 * @Fields CoverImage :背景图片
		 */
		public String CoverImage;
		/**
		 * @Fields SubmitStatus : 提交状态 商户版
		 */
		public String SubmitStatus;
		/**
		 * @Fields Attend : 关注
		 */
		public String Attend;
		/**
		 * @Fields GradePercent : 等级百分比
		 */
		public String GradePercent;
		/**
		 * @Fields TCurrencyDistance :距离升级的点数 
		 */
		public String TCurrencyDistance;
		/**
		 * @Fields Fans : 粉丝
		 */
		public String Fans;
		/**
		 * @Fields Relation : 商户版用的
		 */
		public String Relation;
		
		public String Source;

		@Override
		public String toString() {
			return "UserInfo [UserId=" + UserId + ", UserName=" + UserName
					+ ", LogoUrl=" + LogoUrl + ", TCurrency=" + TCurrency
					+ ", PublishStory=" + PublishStory + ", BusinessStatus="
					+ BusinessStatus + ", UserGrade=" + UserGrade
					+ ", Verified=" + Verified + ", CoverImage=" + CoverImage
					+ ", SubmitStatus=" + SubmitStatus + ", Attend=" + Attend
					+ ", GradePercent=" + GradePercent + ", TCurrencyDistance="
					+ TCurrencyDistance + ", Fans=" + Fans + ", Relation="
					+ Relation + ", Source=" + Source + "]";
		}
		
		

	}

}
