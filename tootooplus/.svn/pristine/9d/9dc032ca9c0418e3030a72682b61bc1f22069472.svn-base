<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical">
    <include
    layout="@layout/two_or_one_btn_head"/>
    
	<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center_horizontal">
        
        
        <EditText
        android:id="@+id/change_nick_nick"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/mobile_nick_name_hint"
        android:singleLine="true"
        android:background="@drawable/login_nick_et_drawable"
        android:layout_marginLeft="@dimen/login_register_left_right_margin"
        android:layout_marginRight="@dimen/login_register_left_right_margin"
        android:layout_marginTop="15dp"
        android:textSize="@dimen/h3"/>
        
    </LinearLayout>
    

</LinearLayout>
