<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/app_bg_color"
android:orientation="vertical">
    <include
     layout="@layout/two_or_one_btn_head"/>
    <ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
        <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
            <RelativeLayout
		    android:id="@+id/mine_per_info_layout"
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/person_center_first_line_height">
		        <ImageView
		        android:id="@+id/mine_per_info_cover_iv"
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:scaleType="fitXY"/>
		        
		        <LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_alignParentBottom="true"
				android:background="@drawable/bg_user_info_head_bottom">
				    
				</LinearLayout>
		        
			    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:orientation="vertical"
		        android:layout_alignBottom="@id/mine_per_info_cover_iv"
		        android:weightSum="6">
		        	<LinearLayout
		        	android:layout_width="match_parent"
		        	android:layout_height="0dp"
		        	android:layout_weight="4"
		        	android:gravity="bottom|center_horizontal"
		        	android:orientation="horizontal">
			        	<RelativeLayout
			            android:id="@+id/per_center_photo_layout"
		        	    android:layout_width="@dimen/mine_photo_width_height"
		            	android:layout_height="@dimen/mine_photo_width_height">
		            	
		        	        <ImageView
				           	android:id="@+id/person_center_photo"
				            android:layout_width="@dimen/mine_photo_width_height"
				            android:layout_height="@dimen/mine_photo_width_height"
				            android:src="@drawable/icon_user_large_logo" />
		        	        
		        	        <ImageView
				           	android:id="@+id/person_center_vip"
				            android:layout_width="wrap_content"
				            android:layout_height="wrap_content"
				            android:src="@drawable/icon_vip_1"
				            android:layout_alignParentRight="true"
				            android:layout_alignParentBottom="true"/>
		        	        
		        	    </RelativeLayout>
		        	    
		        	</LinearLayout>
			        <LinearLayout
		            android:layout_width="match_parent"
		            android:layout_height="0dp"
		            android:layout_weight="1"
		            android:orientation="horizontal"
		            android:gravity="center">
		                <TextView
		                android:id="@+id/person_center_name"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:singleLine="true"
		                android:textColor="@color/white"
		                android:textSize="@dimen/h2" />
		                
		            </LinearLayout>
		            
		            <LinearLayout
		            android:layout_width="match_parent"
		            android:layout_height="0dp"
		            android:layout_weight="1"
		            android:gravity="center"
		            android:orientation="horizontal">
		                 <TextView
		                android:id="@+id/mine_store_id"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:textColor="@color/white"
		                android:text="ID"/>
		                
		            </LinearLayout>
		        </LinearLayout>
		    </RelativeLayout>
		    
		    
		    <View
		    android:layout_width="match_parent"
		    android:layout_height="0.5dp"
		    android:background="@color/title_hor_line"/>
		    <LinearLayout
		    android:id="@+id/mine_act_apply_layout"
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/tab_item_hight"
		    android:orientation="horizontal"
		    android:background="@color/white"
		    android:gravity="center_vertical"
		    android:weightSum="5">
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="0.5"
		        android:gravity="center_vertical"
		        android:layout_marginLeft="10dp"
		        android:orientation="horizontal" >
		
		            <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/icon_mine_free_act" />
		         </LinearLayout>
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="4"
		        android:gravity="center_vertical"
		        android:orientation="horizontal" >
		
		            <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginLeft="5dp"
		            android:text="@string/mine_act_apply" />
		         </LinearLayout>
		         <LinearLayout
			 	    android:layout_width="0dip"
			 	    android:layout_height="fill_parent"
			 	    android:orientation="horizontal"
			 	    android:layout_weight="0.5"
			 	    android:gravity="center">
			 	        <ImageView
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:src="@drawable/icon_arrow"/>
		 	        </LinearLayout>
		    </LinearLayout>
		    <View
		    android:layout_width="match_parent"
		    android:layout_height="0.5dp"
		    android:background="@color/title_hor_line"/>
		    
		    
		    <LinearLayout
		    android:id="@+id/mine_sj_layout"
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/tab_item_hight"
		    android:orientation="horizontal"
		    android:background="@color/white"
		    android:gravity="center_vertical"
		    android:weightSum="5">
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="0.5"
		        android:gravity="center_vertical"
		        android:layout_marginLeft="10dp"
		        android:orientation="horizontal" >
		
		            <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/icon_mine_sj" />
		         </LinearLayout>
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="4"
		        android:gravity="center_vertical"
		        android:orientation="horizontal" >
		
		            <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginLeft="5dp"
		            android:text="@string/mine_sj" />
		         </LinearLayout>
		         <LinearLayout
			 	    android:layout_width="0dip"
			 	    android:layout_height="fill_parent"
			 	    android:orientation="horizontal"
			 	    android:layout_weight="0.5"
			 	    android:gravity="center">
			 	        <ImageView
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:src="@drawable/icon_arrow"/>
		 	        </LinearLayout>
		    </LinearLayout>
			<View
		    android:layout_width="match_parent"
		    android:layout_height="0.5dp"
		    android:background="@color/title_hor_line"/>
		    
		    
		    
		    
		    <LinearLayout
		    android:id="@+id/mine_msg_layout"
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/tab_item_hight"
		    android:orientation="horizontal"
		    android:background="@color/white"
		    android:gravity="center_vertical"
		    android:weightSum="5">
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="0.5"
		        android:gravity="center_vertical"
		        android:layout_marginLeft="10dp"
		        android:orientation="horizontal" >
		
		            <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/icon_mine_my_msg" />
		         </LinearLayout>
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="4"
		        android:gravity="center_vertical"
		        android:orientation="horizontal" >
		
		            <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginLeft="5dp"
		            android:text="@string/mine_msg" />
		         </LinearLayout>
		         <LinearLayout
			 	    android:layout_width="0dip"
			 	    android:layout_height="fill_parent"
			 	    android:orientation="horizontal"
			 	    android:layout_weight="0.5"
			 	    android:gravity="center">
			 	        <ImageView
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:src="@drawable/icon_arrow"/>
		 	        </LinearLayout>
		    </LinearLayout>
			<View
		    android:layout_width="match_parent"
		    android:layout_height="0.5dp"
		    android:background="@color/title_hor_line"/>
			
			<LinearLayout
		    android:id="@+id/mine_recommend_layout"
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/tab_item_hight"
		    android:orientation="horizontal"
		    android:background="@color/white"
		    android:gravity="center_vertical"
		    android:weightSum="5">
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="0.5"
		        android:gravity="center_vertical"
		        android:layout_marginLeft="10dp"
		        android:orientation="horizontal" >
		
		            <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/icon_mine_my_wish" />
		         </LinearLayout>
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="4"
		        android:gravity="center_vertical"
		        android:orientation="horizontal" >
		
		            <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginLeft="5dp"
		            android:text="@string/mine_recommend" />
		         </LinearLayout>
		         <LinearLayout
			 	    android:layout_width="0dip"
			 	    android:layout_height="fill_parent"
			 	    android:orientation="horizontal"
			 	    android:layout_weight="0.5"
			 	    android:gravity="center">
			 	        <ImageView
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:src="@drawable/icon_arrow"/>
		 	        </LinearLayout>
		    </LinearLayout>
			<View
		    android:layout_width="match_parent"
		    android:layout_height="0.5dp"
		    android:background="@color/title_hor_line"/>
			
			
			<LinearLayout
		    android:id="@+id/mine_change_pwd_layout"
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/tab_item_hight"
		    android:orientation="horizontal"
		    android:background="@color/white"
		    android:gravity="center_vertical"
		    android:weightSum="5">
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="0.5"
		        android:gravity="center_vertical"
		        android:layout_marginLeft="10dp"
		        android:orientation="horizontal" >
		
		            <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/icon_mine_change_pwd" />
		         </LinearLayout>
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="4"
		        android:gravity="center_vertical"
		        android:orientation="horizontal" >
		
		            <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginLeft="5dp"
		            android:text="@string/mine_change_pwd" />
		         </LinearLayout>
		         <LinearLayout
			 	    android:layout_width="0dip"
			 	    android:layout_height="fill_parent"
			 	    android:orientation="horizontal"
			 	    android:layout_weight="0.5"
			 	    android:gravity="center">
			 	        <ImageView
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:src="@drawable/icon_arrow"/>
		 	        </LinearLayout>
		    </LinearLayout>
		    
			<View
		    android:layout_width="match_parent"
		    android:layout_height="0.5dp"
		    android:background="@color/title_hor_line"/>
			
			<LinearLayout
		    android:id="@+id/mine_set_layout"
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/tab_item_hight"
		    android:orientation="horizontal"
		    android:background="@color/white"
		    android:gravity="center_vertical"
		    android:weightSum="5">
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="0.5"
		        android:gravity="center_vertical"
		        android:layout_marginLeft="10dp"
		        android:orientation="horizontal" >
		
		            <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/icon_mine_set" />
		         </LinearLayout>
		        <LinearLayout
		        android:layout_width="0dip"
		        android:layout_height="fill_parent"
		        android:layout_weight="4"
		        android:gravity="center_vertical"
		        android:orientation="horizontal" >
		
		            <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginLeft="5dp"
		            android:text="@string/mine_set" />
		         </LinearLayout>
		         <LinearLayout
			 	    android:layout_width="0dip"
			 	    android:layout_height="fill_parent"
			 	    android:orientation="horizontal"
			 	    android:layout_weight="0.5"
			 	    android:gravity="center">
			 	        <ImageView
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:src="@drawable/icon_arrow"/>
		 	        </LinearLayout>
		    </LinearLayout>
			<View
		    android:layout_width="match_parent"
		    android:layout_height="0.5dp"
		    android:background="@color/title_hor_line"/>
            
            
            
            
        </LinearLayout>
        
    </ScrollView>
	
</LinearLayout>
