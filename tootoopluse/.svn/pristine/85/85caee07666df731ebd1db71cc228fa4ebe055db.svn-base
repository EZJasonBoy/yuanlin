package com.wiriamubin.service.chat;

import java.io.Serializable;
/** 
* @ClassName: ChatMessage 
* @Description: 聊天室消息实体类
* @author zhou
* @date 2015-1-26 下午7:26:55 
*  
*/
public class Messages implements Serializable{
	
	public String Type;
	public String Content;
	@Override
	public String toString() {
		return "Message [Type=" + Type + ", Content=" + Content + "]";
	}
	public Messages(String type, String content) {
		super();
		Type = type;
		Content = content;
	}

}
