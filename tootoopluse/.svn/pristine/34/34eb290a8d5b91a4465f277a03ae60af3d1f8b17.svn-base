<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:orientation="vertical">
    <LinearLayout
   	android:layout_width="match_parent"
   	android:layout_height="@dimen/tab_item_hight"
   	android:weightSum="7"
   	android:orientation="horizontal">
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="2"
   	    android:gravity="center"
   	    android:orientation="horizontal">
   	        <TextView
   	        android:layout_width="wrap_content"
   	        android:layout_height="wrap_content"
   	        android:text="@string/create_act_send_area"/>
   	        
   	    </LinearLayout>
   	    
   	    
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="4"
   	    android:paddingLeft="5dp"
   	    android:gravity="center_vertical"
   	    android:orientation="horizontal">
  	        <LinearLayout
   	        android:id="@+id/create_act_online_change_send_layout"
   	        android:layout_width="match_parent"
   	        android:layout_height="@dimen/create_act_frist_step_story_height"
   	        android:background="@drawable/bg_white_corners_shape_drawable"
   	        android:weightSum="6"
   	        android:gravity="center_vertical"
   	        android:orientation="horizontal">
   	            <TextView
    	        android:id="@+id/create_act_online_change_send_area"
                android:layout_width="0dp"
                android:layout_weight="5"
                android:singleLine="true"
                android:padding="5dp"
                android:layout_height="wrap_content"/>
    	        <LinearLayout
    	        android:layout_width="0dp"
    	        android:layout_weight="1"
    	        android:layout_height="match_parent"
    	        android:orientation="horizontal"
    	        android:gravity="center">
    	            <ImageView
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:src="@drawable/icon_arrow_down"/>
    	        </LinearLayout>
   	        
  	       </LinearLayout>
   	        
   	    </LinearLayout>
   	    
   	     <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="1"
   	    android:paddingLeft="5dp"
   	    android:gravity="center_vertical"
   	    android:orientation="horizontal">
  	        
   	         <TextView
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:singleLine="true"
             android:paddingLeft="5dp"
             android:text="@string/create_act_third_arrive"/>
  	    </LinearLayout>
   	    
   	</LinearLayout>

    <LinearLayout
   	android:layout_width="match_parent"
   	android:layout_height="@dimen/tab_item_hight"
   	android:weightSum="7"
   	android:orientation="horizontal">
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="2"
   	    android:gravity="center"
   	    android:orientation="horizontal">
   	       <!-- 空 -->
   	        
   	    </LinearLayout>
   	    
   	    
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="4"
   	    android:paddingLeft="5dp"
   	    android:gravity="center_vertical"
   	    android:orientation="horizontal">
  	        <LinearLayout
   	        android:id="@+id/create_act_online_change_arrive_layout"
   	        android:layout_width="match_parent"
   	        android:layout_height="@dimen/create_act_frist_step_story_height"
   	        android:background="@drawable/bg_white_corners_shape_drawable"
   	        android:weightSum="6"
   	        android:gravity="center_vertical"
   	        android:orientation="horizontal">
   	            <TextView
    	        android:id="@+id/create_act_online_change_arrive_area"
                android:layout_width="0dp"
                android:layout_weight="5"
                android:singleLine="true"
                android:padding="5dp"
                android:maxEms="4"
                android:layout_height="wrap_content"/>
    	        <LinearLayout
    	        android:layout_width="0dp"
    	        android:layout_weight="1"
    	        android:layout_height="match_parent"
    	        android:orientation="horizontal"
    	        android:gravity="center">
    	            <ImageView
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:src="@drawable/icon_arrow_down"/>
    	        </LinearLayout>
   	        
  	       </LinearLayout>
   	        
   	    </LinearLayout>
   	    
   	</LinearLayout>
    
    <LinearLayout
   	android:layout_width="match_parent"
   	android:layout_height="@dimen/tab_item_hight"
   	android:weightSum="7"
   	android:orientation="horizontal">
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="2"
   	    android:gravity="center"
   	    android:orientation="horizontal">
   	        <TextView
   	        android:layout_width="wrap_content"
   	        android:layout_height="wrap_content"
   	        android:text="@string/create_act_send_type"/>
   	        
   	    </LinearLayout>
   	    
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="4"
   	    android:gravity="center_vertical"
   	    android:paddingLeft="5dp"
   	    android:orientation="horizontal">
   	        
   	        <LinearLayout
   	        android:id="@+id/create_act_third_step_send_type_layout"
   	        android:layout_width="match_parent"
   	        android:layout_height="@dimen/create_act_frist_step_story_height"
   	        android:background="@drawable/bg_white_corners_shape_drawable"
   	        android:weightSum="6"
   	        android:gravity="center_vertical"
   	        android:orientation="horizontal">
   	            <TextView
    	        android:id="@+id/create_act_online_change_send_type"
                android:layout_width="0dp"
                android:layout_weight="5"
                android:paddingLeft="5dp"
                android:layout_height="wrap_content"
                android:text="@string/create_act_send_tootoo"/>
    	        <LinearLayout
    	        android:layout_width="0dp"
    	        android:layout_weight="1"
    	        android:layout_height="match_parent"
    	        android:orientation="horizontal"
    	        android:gravity="center">
    	            <ImageView
    	            android:id="@+id/create_act_online_change_send_arrow"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:src="@drawable/icon_arrow_down"/>
    	        </LinearLayout>
    	        
   	        </LinearLayout>
   	        
   	        
   	    </LinearLayout>
   	    
   	</LinearLayout>
   	
    
    
    <LinearLayout
    android:id="@+id/create_act_online_change_other_send_type_layout"
   	android:layout_width="match_parent"
   	android:layout_height="@dimen/tab_item_hight"
   	android:weightSum="7"
   	android:orientation="horizontal">
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="2"
   	    android:gravity="center"
   	    android:orientation="horizontal">
   	        
   	        <!-- 空 -->
   	        
   	    </LinearLayout>
   	    
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="4"
   	    android:gravity="center_vertical"
   	    android:paddingLeft="5dp"
   	    android:orientation="horizontal">
   	        <EditText
   	        android:id="@+id/create_act_online_change_other_send_type_et"
            android:layout_width="match_parent"
            android:layout_height="@dimen/create_act_frist_step_story_height"
            android:singleLine="true"
            android:paddingLeft="5dp"
            android:textSize="@dimen/h4"
            android:hint="@string/create_act_send_type_hint"
            android:background="@drawable/bg_app_color_corners_shape_drawable"/>
   	        
   	        
   	    </LinearLayout>
   	    
   	    <LinearLayout
   	    android:layout_width="0dp"
   	    android:layout_height="match_parent"
   	    android:layout_weight="1"
   	    android:gravity="center_vertical"
   	    android:orientation="horizontal">
   	        
   	        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:paddingLeft="5dp"
            android:text="@string/create_act_select_or_no_hint"/>
   	    </LinearLayout>
   	    
   	</LinearLayout>
    
    
</LinearLayout>
