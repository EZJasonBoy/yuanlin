<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <ImageView 
        android:id="@+id/iv_photo_rember"
        android:layout_width="@dimen/photo_diment"
        android:layout_height="@dimen/photo_diment"
        />
    <CheckedTextView 
        android:id="@+id/tv_user_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/TvStyleDefault"
        android:textSize="@dimen/h3"
        android:layout_below="@id/iv_photo_rember"
        android:textColor="@color/black"
        />
      <CheckedTextView 
        android:id="@+id/tv_user_member"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/TvStyleDefault"
        android:textSize="@dimen/h3"
        android:textColor="@color/black"
        android:layout_below="@id/tv_user_name"
        />
        
    

</RelativeLayout>
