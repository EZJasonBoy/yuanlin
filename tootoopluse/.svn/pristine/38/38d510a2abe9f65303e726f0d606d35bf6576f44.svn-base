package com.ninetowns.tootoopluse.bean;

public class LoginBean {
	
	//登录返回的成功或者失败状态
	private String login_status;
	
	//登录用户来源
	private String login_source;
	
	//登录第三方id
	private String login_relationId;
	
	//登录的id
	private String login_Id;
	
	//登录的用户名
	private String login_name;
	
	//登录用户的头像地址
	private String login_logoUrl;
	
	//登录状态，暂时用不到
	private String login_type;
	
	//是否有权限发布故事
	private String login_publishStory;
	
	//是否第一次登录
	private String IsOneLogin;
	
	private String login_businessStatus;
	
	/**登录时T币数**/
	private String login_tCurrency;
	
	/**是否乐活达人 "1"表示是乐活达人, "0"表示不是乐活达人**/
	private String login_isLOHAS;
	
	/**用户的各种称号**/
	private String login_medal;
	
	/**用户VIP等级**/
	private String login_userGrade;
	/**个人封面图**/
	private String login_coverImage;
	
	public String getLogin_status() {
		return login_status;
	}

	public void setLogin_status(String login_status) {
		this.login_status = login_status;
	}

	public String getLogin_source() {
		return login_source;
	}

	public void setLogin_source(String login_source) {
		this.login_source = login_source;
	}

	public String getLogin_relationId() {
		return login_relationId;
	}

	public void setLogin_relationId(String login_relationId) {
		this.login_relationId = login_relationId;
	}

	public String getLogin_Id() {
		return login_Id;
	}

	public void setLogin_Id(String login_Id) {
		this.login_Id = login_Id;
	}

	public String getLogin_name() {
		return login_name;
	}

	public void setLogin_name(String login_name) {
		this.login_name = login_name;
	}

	public String getLogin_logoUrl() {
		return login_logoUrl;
	}

	public void setLogin_logoUrl(String login_logoUrl) {
		this.login_logoUrl = login_logoUrl;
	}

	public String getLogin_type() {
		return login_type;
	}

	public void setLogin_type(String login_type) {
		this.login_type = login_type;
	}

	public String getLogin_publishStory() {
		return login_publishStory;
	}

	public void setLogin_publishStory(String login_publishStory) {
		this.login_publishStory = login_publishStory;
	}

	public String getIsOneLogin() {
		return IsOneLogin;
	}

	public void setIsOneLogin(String isOneLogin) {
		IsOneLogin = isOneLogin;
	}

	public String getLogin_businessStatus() {
		return login_businessStatus;
	}

	public void setLogin_businessStatus(String login_businessStatus) {
		this.login_businessStatus = login_businessStatus;
	}

	public String getLogin_tCurrency() {
		return login_tCurrency;
	}

	public void setLogin_tCurrency(String login_tCurrency) {
		this.login_tCurrency = login_tCurrency;
	}

	public String getLogin_isLOHAS() {
		return login_isLOHAS;
	}

	public void setLogin_isLOHAS(String login_isLOHAS) {
		this.login_isLOHAS = login_isLOHAS;
	}

	public String getLogin_medal() {
		return login_medal;
	}

	public void setLogin_medal(String login_medal) {
		this.login_medal = login_medal;
	}

	public String getLogin_userGrade() {
		return login_userGrade;
	}

	public void setLogin_userGrade(String login_userGrade) {
		this.login_userGrade = login_userGrade;
	}


	public String getLogin_coverImage() {
		return login_coverImage;
	}

	public void setLogin_coverImage(String login_coverImage) {
		this.login_coverImage = login_coverImage;
	}

	@Override
	public String toString() {
		return "LoginBean [login_status=" + login_status + ", login_source="
				+ login_source + ", login_relationId=" + login_relationId
				+ ", login_Id=" + login_Id + ", login_name=" + login_name
				+ ", login_logoUrl=" + login_logoUrl + ", login_type="
				+ login_type + ", login_publishStory=" + login_publishStory
				+ ", IsOneLogin=" + IsOneLogin + ", login_businessStatus="
				+ login_businessStatus + ", login_tCurrency=" + login_tCurrency
				+ ", login_isLOHAS=" + login_isLOHAS + ", login_medal="
				+ login_medal + ", login_userGrade=" + login_userGrade
				+ ", login_coverImage=" + login_coverImage + "]";
	}

}
