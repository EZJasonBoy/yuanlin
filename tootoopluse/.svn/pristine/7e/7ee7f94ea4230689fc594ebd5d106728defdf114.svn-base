<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/app_bg_color"
android:orientation="vertical">
    
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/title_bar_height"
    android:background="@drawable/bg_title_shape_drawable"
    android:orientation="horizontal" >

        <LinearLayout
         android:id="@+id/two_or_one_btn_head_back"
         android:layout_width="0dp"
         android:layout_height="match_parent"
         android:layout_weight="1"
         android:gravity="center"
         android:orientation="horizontal" >

            <TextView
                android:id="@+id/two_or_one_iv_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/create_act_last_step"
                android:textSize="@dimen/h2" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:gravity="center"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/two_or_one_btn_head_title"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/h1" />
            
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:gravity="center"
                android:textSize="@dimen/title_num_dimen"
                android:textColor="@color/red"
                android:text="@string/create_act_title_third" />
            
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="@dimen/h3"
                android:text="@string/create_act_title_total_step" />
            
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/create_act_third_step_submit_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/two_or_one_btn_head_second_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone" />

            <TextView
                android:id="@+id/two_or_one_btn_head_second_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/h2"
                android:visibility="gone" />
        </RelativeLayout>
    </LinearLayout>
    
    
    
    
    <ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
        <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:background="@color/white"
	    android:orientation="vertical">
	    
	    	<LinearLayout
	    	android:layout_width="match_parent"
	    	android:layout_height="@dimen/tab_item_hight"
	    	android:weightSum="7"
	    	android:orientation="horizontal">
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="2"
	    	    android:gravity="center"
	    	    android:orientation="horizontal">
	    	        <TextView
	    	        android:layout_width="wrap_content"
	    	        android:layout_height="wrap_content"
	    	        android:text="@string/create_act_type"/>
	    	        
	    	    </LinearLayout>
	    	    
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="4"
	    	    android:paddingLeft="5dp"
	    	    android:orientation="horizontal"
	    	    android:gravity="center_vertical">
	    	        <LinearLayout
	    	        android:id="@+id/create_act_third_step_line_type_layout"
	    	        android:layout_width="match_parent"
	    	        android:layout_height="@dimen/create_act_frist_step_story_height"
	    	        android:weightSum="6"
	    	        android:paddingLeft="5dp"
	    	        android:background="@drawable/bg_white_corners_shape_drawable"
	    	        android:gravity="center_vertical"
	    	        android:orientation="horizontal">
	    	            <TextView
		    	        android:id="@+id/create_act_third_line_type"
		                android:layout_width="0dp"
		                android:layout_weight="5"
		                android:layout_height="wrap_content"/>
		    	        <LinearLayout
		    	        android:layout_width="0dp"
		    	        android:layout_weight="1"
		    	        android:layout_height="match_parent"
		    	        android:orientation="horizontal"
		    	        android:gravity="center">
		    	            <ImageView
			                android:id="@+id/create_act_third_arrow"
			                android:layout_width="wrap_content"
			                android:layout_height="wrap_content"
			                android:src="@drawable/icon_arrow_down"/>
		    	        </LinearLayout>
		    	        
	    	        </LinearLayout>
	    	        
	    	    </LinearLayout>
	    	    
	    	</LinearLayout>
	        
	        <LinearLayout
	    	android:layout_width="match_parent"
	    	android:layout_height="@dimen/tab_item_hight"
	    	android:weightSum="7"
	    	android:orientation="horizontal">
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="2"
	    	    android:gravity="center"
	    	    android:orientation="horizontal">
	    	        <TextView
	    	        android:layout_width="wrap_content"
	    	        android:layout_height="wrap_content"
	    	        android:text="@string/create_act_person_num"/>
	    	        
	    	    </LinearLayout>
	    	    
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="4"
	    	    android:gravity="center_vertical"
	    	    android:paddingLeft="5dp"
	    	    android:orientation="horizontal">
	    	        <EditText
	    	        android:id="@+id/create_act_third_person_num"
	                android:layout_width="match_parent"
	                android:layout_height="@dimen/create_act_frist_step_story_height"
	                android:background="@drawable/bg_app_color_corners_shape_drawable"
	                android:singleLine="true"
	                android:paddingLeft="5dp"
	                android:textSize="@dimen/h4"/>
	    	        
	    	    </LinearLayout>
	    	    
	    	</LinearLayout>
	        
	        <LinearLayout
	    	android:layout_width="match_parent"
	    	android:layout_height="@dimen/tab_item_hight"
	    	android:weightSum="7"
	    	android:orientation="horizontal">
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="2"
	    	    android:gravity="center"
	    	    android:orientation="horizontal">
	    	        <TextView
	    	        android:layout_width="wrap_content"
	    	        android:layout_height="wrap_content"
	    	        android:text="@string/create_act_enter_time"/>
	    	        
	    	    </LinearLayout>
	    	    
	    	    
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="4"
	    	    android:gravity="center_vertical"
	    	    android:paddingLeft="5dp"
	    	    android:orientation="horizontal">
	    	        <TextView
	    	        android:id="@+id/create_act_third_enter_start_time"
	                android:layout_width="match_parent"
	                android:layout_height="@dimen/create_act_frist_step_story_height"
	                android:background="@drawable/bg_app_color_corners_shape_drawable"
	                android:singleLine="true"
	                android:paddingLeft="5dp"
	                android:gravity="center_vertical"
	                android:hint="@string/start_time_hint"
	                android:textSize="@dimen/h4"/>
	    	        
	    	        
	    	    </LinearLayout>
	    	    
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="1"
	    	    android:gravity="center_vertical"
	    	    android:orientation="horizontal">
	    	        <TextView
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:singleLine="true"
	                android:paddingLeft="5dp"
	                android:text="@string/create_act_third_arrive"/>
	    	    </LinearLayout>
	    	    
	    	</LinearLayout>
	        
	        <LinearLayout
	    	android:layout_width="match_parent"
	    	android:layout_height="@dimen/tab_item_hight"
	    	android:weightSum="7"
	    	android:orientation="horizontal">
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="2"
	    	    android:gravity="center"
	    	    android:orientation="horizontal">
	    	        <!-- ç©º -->
	    	    </LinearLayout>
	    	    
	    	    <LinearLayout
	    	    android:layout_width="0dp"
	    	    android:layout_height="match_parent"
	    	    android:layout_weight="4"
	    	    android:gravity="center_vertical"
	    	    android:paddingLeft="5dp"
	    	    android:orientation="horizontal">
	    	        <TextView
	    	        android:id="@+id/create_act_third_enter_end_time"
	                android:layout_width="match_parent"
	                android:layout_height="@dimen/create_act_frist_step_story_height"
	                android:background="@drawable/bg_app_color_corners_shape_drawable"
	                android:hint="@string/end_time_hint"
	                android:gravity="center_vertical"
	                android:singleLine="true"
	                android:paddingLeft="5dp"
	                android:textSize="@dimen/h4"/>
	    	        
	    	    </LinearLayout>
	    	    
	    	</LinearLayout>
	        
	        <com.ninetowns.tootoopluse.helper.CreateActThirdChangeView
	        android:id="@+id/create_act_third_change_view"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="vertical"/>
	    </LinearLayout>
    </ScrollView>
    
    

</LinearLayout>
