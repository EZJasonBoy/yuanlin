<?xml version="1.0" encoding="utf-8"?>
<project
    name="custom"
    default="channels" >

    <target name="umeng" >

        <replaceregexp
            encoding="utf-8"
            file="AndroidManifest.xml"
            match="&lt;meta\-data(\s+)android:name=&quot;UMENG_CHANNEL&quot;(\s+)android:value=&quot;[a-zA-Z0-9]+&quot;"
            replace="&lt;meta\-data android:name=&quot;UMENG_CHANNEL&quot; android:value=&quot;${UMENG_CHANNEL}&quot;" />

        <delete file="bin/AndroidManifest.xml" />

        <antcall target="release" >
        </antcall>

        <copy
            file="bin/StartActivity-release.apk"
            tofile="bin/channels/tootoopluse-${UMENG_CHANNEL}.apk" >
        </copy>
    </target>

    <target name="channels" >

        <antcall target="clean" >
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="tootoo" />
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="xiaomi" />
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="jifeng" />
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="lianxiang" />
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="baidu" />
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="360yingyongbao" />
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="wandoujia" />
        </antcall>

        <antcall target="umeng" >

            <param
                name="UMENG_CHANNEL"
                value="tengxunyingyongbao" />
        </antcall>
    </target>

</project>